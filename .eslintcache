s",["114","115"],"/Users/mak/Documents/Projects/zori-zavod/src/components/gasForm/gasForm.js",[],["116","117"],"/Users/mak/Documents/Projects/zori-zavod/src/components/electricityForm/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/electricityForm/electricityForm.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/services/zori-zavod-service.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/panel/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/panel/panel.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/item-list/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/item-list/item-list.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/spinner/spinner.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/technology-page/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/technology-page/technology-page.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/item-list-operation/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/item-list-operation/item-list-operation.js",["118"],"import React, { Component } from 'react';\n\nimport './item-list-operation.css';\nimport ZoriZavodService from \"../../services/zori-zavod-service\";\nimport Spinner from \"../spinner/spinner\";\n\nexport default class ItemListOperation extends Component {\n\n    zoriZavodService = new ZoriZavodService()\n    constructor(props) {\n        super(props);\n\n        this.state = {\n            operationList: null,\n            operationsId: [],\n        }\n\n        this.handleCheckboxChange = this.handleCheckboxChange.bind(this)\n    }\n\n    handleCheckboxChange = e => {\n\n        let newArray = [...this.state.operationsId, e.target.id];\n        if (this.state.operationsId.includes(e.target.id)) {\n            newArray = newArray.filter(day => day !== e.target.id);\n        }\n        this.setState({\n            operationsId: newArray\n        });\n    }\n\n    // onTechnologySelected = (selectedTechnology) => {\n    //     this.setState({selectedTechnology})\n    // }\n\n    componentDidMount() {\n        this.updateOperation()\n    }\n\n    componentDidUpdate(prevProps) {\n        if (this.props.technologyId !== prevProps.technologyId) {\n            this.updateOperation()\n        }\n    }\n\n    updateOperation() {\n        const { technologyId } = this.props\n        if (!technologyId) {\n            return\n        }\n\n        this.zoriZavodService\n            .getAllOperationsTech(technologyId)\n            .then((operationList) => {\n                this.setState({ operationList })\n            })\n    }\n\n\n    renderItems(arr) {\n\n        return arr.map(({id, name, standard, ratio}) => {\n            return (\n                <div>\n\n                    <li className=\"list-group-item\"\n                        // key={id}\n                        // id={id}\n                        // onClick={() => this.props.onItemSelected(id)}\n                    >\n                        <input type='checkbox' className='checkbox'\n                               id={id}\n                               value={id}\n                               onChange={this.handleCheckboxChange}\n                        />\n                        &nbsp; {name} ( Стандарт: {standard} ) ( Ратио: {ratio} )\n                    </li>\n                </div>\n            );\n        });\n    }\n\n\n    submitHandler = (e) => {\n        alert(`Операции успешно добавлены!`)\n        e.preventDefault();\n        fetch(`http://springreact2.eba-dup8x69j.eu-central-1.elasticbeanstalk.com/api/record/add/`, {\n            method: \"POST\",\n            headers: {\n                'Accept': 'application/json',\n                'Content-Type': 'application/json',\n            },\n            body: JSON.stringify(\n                this.state.operationsId\n            )\n        }).then(response => response.json())\n    }\n\n    render() {\n\n        const { operationList } = this.state\n\n        if (!operationList) {\n            return <span></span>\n        }\n\n        const items = this.renderItems(operationList)\n\n        return (\n            <div>\n\n                <form onSubmit={this.submitHandler}>\n\n                    <ul className='item-list list-group'>\n                        {items}\n                    </ul>\n\n                    <button type='submit' className='btn btn-success' >Отправить</button>\n                </form>\n            </div>\n        )\n    }\n}\n\n\n\n\n\n\n","/Users/mak/Documents/Projects/zori-zavod/src/components/technology-details/technology-details.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/history-item-list/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/history-item-list/history-item-list.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/post-request-tech/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/post-request-tech/post-request-tech.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/post-request-operation/post-request-operation.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/post-request-operation/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/modal-window/index.js",[],"/Users/mak/Documents/Projects/zori-zavod/src/components/modal-window/add-new-techn-modal.js",[],{"ruleId":"119","replacedBy":"120"},{"ruleId":"121","replacedBy":"122"},{"ruleId":"119","replacedBy":"123"},{"ruleId":"121","replacedBy":"124"},{"ruleId":"125","severity":1,"message":"126","line":2,"column":8,"nodeType":"127","messageId":"128","endLine":2,"endColumn":18},{"ruleId":"125","severity":1,"message":"129","line":4,"column":8,"nodeType":"127","messageId":"128","endLine":4,"endColumn":25},{"ruleId":"119","replacedBy":"130"},{"ruleId":"121","replacedBy":"131"},{"ruleId":"125","severity":1,"message":"132","line":5,"column":8,"nodeType"